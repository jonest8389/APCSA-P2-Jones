import static java.lang.System.*;

public class Lab11c
{
   public static void main( String args[] )
   {

	   TriangleThree test = new TriangleThree(3,"A");
	   out.println(test);

	   test.setTriangle("X",7);
	   out.println(test);
	   
	   test.setTriangle("R",1);
	   out.println(test);
	   
	   test.setTriangle("T",5);
	   out.println(test);

	   test.setTriangle("W",4);
	   out.println(test);
	   
	   test.setTriangle("O",40);
	   out.println(test);
	}
}