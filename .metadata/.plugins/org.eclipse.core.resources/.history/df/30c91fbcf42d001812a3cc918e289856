import java.io.File;
import java.io.IOException;
import java.util.Scanner;
import static java.lang.System.*;

public class Lab21i
{
	public static void main( String args[] ) throws IOException
	{
		Scanner file = new Scanner(new File("C:\\Users\\jonest8389\\Desktop\\APCSA-P2-Jones\\Unit14\\src\\lab21i.dat"));
		int maze1size = Integer.parseInt(file.nextLine());
		int[][] maze1 = new int[maze1size][maze1size];
		String mazeline = file.nextLine();
		int j = 0;
		for (int i = 0; i < maze1size - 1; i++)
		{
			maze1[i][j] = Integer.parseInt(Character.toString(mazeline.charAt(j)));
			j = j + 2;
		}
		for (int k = 0;k < maze1size - 1;k++)
		{
			for (int l = 0; l < maze1size - 1;l++)
			{
				out.println(maze1[k][l]);
			}
		}
		
		
		
	}
}